  },
  "files": [
    "index-browser.mjs",
    "index-node.js",
    "xmllint-browser.mjs",
    "xmllint-node.js",
    "xmllint.wasm",
    "index-browser.d.ts",
    "index-browser.mjs.flow",
    "index-node.d.ts",
    "index-node.js.flow"
  ],
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-transform-flow-strip-types": "^7.19.0",
